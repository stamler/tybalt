{
  "name": "tybalt-frontend",
  "version": "0.13.24",
  "private": true,
  "scripts": {
    "copyconfig": "cp ../config.ts src/config.ts",
    "setversion": "node --print \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "prebuild": "npm run copyconfig && npm run setversion",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "mocha"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "jsdom-global/register"
    ],
    "spec": [
      "test/**/*.ts"
    ],
    "exit": true
  },
  "dependencies": {
    "@algolia/autocomplete-js": "^1.4.1",
    "@algolia/autocomplete-theme-classic": "^1.4.1",
    "@types/json2csv": "^5.0.1",
    "@types/lodash": "^4.14.168",
    "@types/marked": "^4.0.8",
    "@types/uuid": "^9.0.1",
    "@vuepic/vue-datepicker": "^3.5.3",
    "algoliasearch": "^4.8.6",
    "core-js": "^3.8.3",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^2.0.0",
    "firebase": "9.16.0",
    "highlight.js": "^11.7.0",
    "js-sha256": "^0.9.0",
    "json2csv": "^5.0.5",
    "marked": "^4.2.12",
    "pinia": "^2.0.23",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "vue": "^3.2.45",
    "vue-instantsearch": "^4",
    "vue-router": "^4.1.6",
    "vuefire": "^3.0.0-alpha.9"
  },
  "devDependencies": {
    "@iconify/vue": "^4.0.2",
    "@types/chai": "^4.3.1",
    "@types/jsdom": "^20.0.0",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.6",
    "diff-package-lock": "^0.1.34",
    "eslint": "^7.19.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^9.7.0",
    "jsdom": "^20.0.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.2.2",
    "prettier": "^2.2.1",
    "sass": "^1.49.7",
    "sass-loader": "^10.1.1",
    "ts-node": "^10.7.0",
    "typescript": "~4.7"
  }
}
