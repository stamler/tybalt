    "firebase-functions": "^3.6.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "decache": "^4.6.0",
    "eslint": "^7.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "firebase-functions-test": "^0.1.6",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2"
